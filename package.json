{
  "name": "highlightmedia-backend",
  "version": "1.0.0",
  "description": "Backend for highlightmedia.in",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "tsx watch src/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:create": "prisma migrate dev --create-only",
    "reset": "prisma migrate reset",
    "generate": "prisma generate",
    "prisma:format": "prisma format",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.957.0",
    "@aws-sdk/client-sns": "^3.888.0",
    "@aws-sdk/lib-storage": "^3.967.0",
    "@aws-sdk/s3-request-presigner": "^3.957.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^7.7.3",
    "@fastify/swagger": "^8.12.0",
    "@fastify/swagger-ui": "^2.1.0",
    "@prisma/client": "^6.19.0",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.24.3",
    "firebase-admin": "^13.6.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.11",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "rate-limiter-flexible": "^3.0.6",
    "razorpay": "^2.9.6",
    "socket.io": "^4.8.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.27",
    "@types/nodemailer": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "prisma": "^6.19.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
